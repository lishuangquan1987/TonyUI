<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Yellow Theme Colors -->
    <Color x:Key="Primary50">#fefce8</Color>
    <Color x:Key="Primary100">#fef9c3</Color>
    <Color x:Key="Primary200">#fef08a</Color>
    <Color x:Key="Primary300">#fde047</Color>
    <Color x:Key="Primary400">#fbcf33</Color>
    <Color x:Key="Primary500">#eab308</Color>
    <Color x:Key="Primary600">#ca8a04</Color>
    <Color x:Key="Primary700">#a16207</Color>
    <Color x:Key="Primary800">#854d0e</Color>
    <Color x:Key="Primary900">#713f12</Color>
    
    <!-- Background Colors -->
    <Color x:Key="Background">#ffffff</Color>
    <Color x:Key="BackgroundMuted">#fefce8</Color>
    <Color x:Key="BackgroundDim">#fef9c3</Color>
    
    <!-- Foreground Colors -->
    <Color x:Key="Foreground">#020817</Color>
    <Color x:Key="ForegroundSecondary">#6b7280</Color>
    <Color x:Key="ForegroundTertiary">#9ca3af</Color>
    
    <!-- Border Colors -->
    <Color x:Key="Border">#fde047</Color>
    <Color x:Key="BorderSecondary">#eab308</Color>
    
    <!-- State Colors -->
    <Color x:Key="Success">#10b981</Color>
    <Color x:Key="Warning">#f59e0b</Color>
    <Color x:Key="Error">#ef4444</Color>
    <Color x:Key="Info">#3b82f6</Color>
    
</ResourceDictionary>