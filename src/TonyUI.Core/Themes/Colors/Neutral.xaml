<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Background">#ffffff</Color>
    <Color x:Key="Foreground">#0a0a0a</Color>

    <Color x:Key="Card">#ffffff</Color>
    <Color x:Key="CardForeground">#0a0a0a</Color>

    <Color x:Key="Popover">#ffffff</Color>
    <Color x:Key="PopoverForeground">#0a0a0a</Color>

    <Color x:Key="Primary">#0a0a0a</Color>
    <Color x:Key="PrimaryForeground">#ffffff</Color>

    <Color x:Key="Secondary">#fafafa</Color>
    <Color x:Key="SecondaryForeground">#0a0a0a</Color>

    <Color x:Key="Muted">#fafafa</Color>
    <Color x:Key="MutedForeground">#737373</Color>

    <Color x:Key="Accent">#fafafa</Color>
    <Color x:Key="AccentForeground">#0a0a0a</Color>

    <Color x:Key="Destructive">#dc2626</Color>
    <Color x:Key="DestructiveForeground">#ffffff</Color>

    <Color x:Key="Border">#e5e5e5</Color>
    <Color x:Key="Input">#e5e5e5</Color>
    <Color x:Key="Ring">#0a0a0a</Color>

</ResourceDictionary>